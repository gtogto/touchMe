<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context=".com.start_Activity">

    <LinearLayout
        android:id="@+id/linearLayout_1"
        android:layout_width="584dp"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="100dp"
            android:text="Touch Me"
            android:textAlignment="center"
            android:textSize="92sp"
            android:textStyle="bold|italic" />

    </LinearLayout>


    <LinearLayout
        android:id="@+id/linearLayout_2"
        android:layout_width="584dp"
        android:layout_height="463dp"
        android:layout_below="@+id/linearLayout_1"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/logo_red"
            android:layout_width="350dp"
            android:layout_height="381dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center"
            android:src="@drawable/ubio_logo" />

        <Button
            android:id="@+id/button"
            android:layout_width="241dp"
            android:layout_height="95dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="172dp"
            android:layout_marginBottom="241dp"
            android:text="로그인"
            android:textSize="40sp" />

    </LinearLayout>

    <RelativeLayout
        android:id="@+id/RelativeLayout_3"
        android:layout_width="558dp"
        android:layout_height="148dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="40dp">


        <TextView
            android:id="@+id/ubio_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="230dp"
            android:layout_marginBottom="23dp"
            android:text="UBIO"
            android:textAllCaps="false"
            android:textSize="84sp"
            android:textStyle="bold|italic" />

        <ImageView
            android:id="@+id/ubio_blue"
            android:layout_width="129dp"
            android:layout_height="118dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="99dp"
            android:src="@drawable/ubio_logo_blue" />

        <TextView
            android:id="@+id/inno_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="168dp"
            android:text="Innovation"
            android:textSize="32sp"
            android:textStyle="bold|italic" />

    </RelativeLayout>


</RelativeLayout>
