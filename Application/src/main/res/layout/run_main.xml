<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <LinearLayout
        android:id="@+id/set_up_main_Linear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">

            <LinearLayout
                android:id="@+id/linearLayout_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginRight="16dp"
                android:gravity="center"
                android:layout_gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/back_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="onClickBack"
                    android:scaleType="fitXY"
                    android:adjustViewBounds="true"
                    android:src="@drawable/back_icon"
                    android:visibility="gone">
                </ImageView>

                <ImageView
                    android:id="@+id/setup_btn"
                    android:layout_width="70dp"
                    android:layout_height="50dp"
                    android:src="@drawable/ubio_logo"></ImageView>

                <TextView
                    android:id="@+id/timeView"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:ems="5"
                    android:gravity="center"
                    android:text="00:00:00:00 "
                    android:textColor="#000000"
                    android:textSize="30dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/run_mode_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="모드 : "
                    android:textStyle="bold|italic" />

                <TextView
                    android:id="@+id/run_mode_change"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ems="2"
                    android:text="자동 "
                    android:textColor="#00FFE5"
                    android:textStyle="bold|italic" />

            </LinearLayout>


            <LinearLayout
                android:id="@id/linearLayout_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/linearLayout_1"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="16dp"
                android:gravity="center"
                android:layout_gravity="center"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/setup_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="RUN "
                    android:textStyle="bold|italic" />
                <!-- android:textSize="45sp"-->

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:scaleType="center"
                    android:src="@drawable/linie_albastra_lr"></ImageView>

            </LinearLayout>

            <LinearLayout
                android:id="@id/linearLayout_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/linearLayout_1"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="15dp"
                android:layout_marginRight="16dp"
                android:gravity="center"
                android:layout_gravity="center"
                android:orientation="vertical">

                <Button
                    android:id="@+id/master"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_alignParentStart="true"
                    android:layout_gravity="center"
                    android:background="@drawable/orange_circle_button"
                    android:gravity="center"
                    android:text="MASTER"
                    android:textSize="22sp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:id="@id/linearLayout_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/linearLayout_1"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="40dp"
                android:layout_marginRight="16dp"
                android:gravity="center"
                android:layout_gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/nodeBtn1"
                    android:layout_width="85dp"
                    android:layout_height="85dp"
                    android:background="@drawable/green_circle_button_off"
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:text="node 1"
                    android:visibility="gone"/>

                <Button
                    android:id="@+id/nodeBtn2"
                    android:layout_width="85dp"
                    android:layout_height="85dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_marginLeft="50dp"
                    android:background="@drawable/green_circle_button_off"
                    android:text="node 2"
                    android:visibility="gone"/>

                <Button
                    android:id="@+id/nodeBtn3"
                    android:layout_width="85dp"
                    android:layout_height="85dp"
                    android:layout_gravity="center"
                    android:background="@drawable/green_circle_button_off"
                    android:gravity="center"
                    android:layout_marginLeft="50dp"
                    android:text="node 3"
                    android:visibility="gone"/>

            </LinearLayout>

            <LinearLayout
                android:id="@id/linearLayout_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/linearLayout_1"
                android:layout_gravity="center"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="16dp"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/node1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:ems="3"
                    android:gravity="center"
                    android:text="00"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    android:visibility="gone"/>

                <TextView
                    android:id="@+id/node2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="200dp"
                    android:ems="3"
                    android:gravity="center"
                    android:text="00"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    android:visibility="gone"/>

            </LinearLayout>

            <LinearLayout
                android:id="@id/linearLayout_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/linearLayout_1"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="16dp"
                android:gravity="center"
                android:layout_gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/nodeBtn4"
                    android:layout_width="85dp"
                    android:layout_height="85dp"
                    android:layout_gravity="center"
                    android:background="@drawable/green_circle_button_off"
                    android:gravity="center"
                    android:text="node 4"
                    android:visibility="gone"/>

                <Button
                    android:id="@+id/nodeBtn5"
                    android:layout_width="85dp"
                    android:layout_height="85dp"
                    android:layout_gravity="center"
                    android:background="@drawable/green_circle_button_off"
                    android:layout_marginLeft="50dp"
                    android:gravity="center"
                    android:text="node 5"
                    android:visibility="gone"/>

                <Button
                    android:id="@+id/nodeBtn6"
                    android:layout_width="85dp"
                    android:layout_height="85dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="50dp"
                    android:background="@drawable/green_circle_button_off"
                    android:gravity="center"
                    android:text="node 6"
                    android:visibility="gone"/>

            </LinearLayout>

            <LinearLayout
                android:id="@id/linearLayout_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/linearLayout_1"
                android:layout_gravity="center"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="16dp"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/node3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:ems="3"
                    android:gravity="center"
                    android:text="00"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    android:visibility="gone"/>

                <TextView
                    android:id="@+id/node4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="200dp"
                    android:ems="3"
                    android:gravity="center"
                    android:text="00"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    android:visibility="gone"/>

            </LinearLayout>

            <LinearLayout
                android:id="@id/linearLayout_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/linearLayout_1"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="16dp"
                android:gravity="center"
                android:layout_gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/nodeBtn7"
                    android:layout_width="85dp"
                    android:layout_height="85dp"
                    android:layout_gravity="center"
                    android:background="@drawable/green_circle_button_off"
                    android:gravity="center"
                    android:text="node 7"
                    android:visibility="gone"/>

                <Button
                    android:id="@+id/nodeBtn8"
                    android:layout_width="85dp"
                    android:layout_height="85dp"
                    android:layout_gravity="center"
                    android:background="@drawable/green_circle_button_off"
                    android:layout_marginLeft="50dp"
                    android:gravity="center"
                    android:text="node 8"
                    android:visibility="gone"/>

                <Button
                    android:id="@+id/nodeBtn9"
                    android:layout_width="85dp"
                    android:layout_height="85dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="50dp"
                    android:background="@drawable/green_circle_button_off"
                    android:gravity="center"
                    android:text="node 9"
                    android:visibility="gone"/>


            </LinearLayout>

            <LinearLayout
                android:id="@id/linearLayout_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/linearLayout_1"
                android:layout_gravity="center"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="16dp"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/node5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:ems="3"
                    android:gravity="center"
                    android:text="00"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    android:visibility="gone"/>

                <TextView
                    android:id="@+id/node6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="100dp"
                    android:ems="3"
                    android:gravity="center"
                    android:text="00"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    android:visibility="gone"/>

            </LinearLayout>

            <LinearLayout
                android:id="@id/linearLayout_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/linearLayout_1"
                android:layout_gravity="center"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="30dp"
                android:layout_marginRight="16dp"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/start_btn"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:onClick="onClick_test_btn"
                    android:background="@drawable/circie_button"
                    android:textColor="#FF0000"
                    android:textStyle="bold"
                    android:textSize="25sp"
                    android:text="START" />

                <Button
                    android:id="@+id/stop_btn"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:onClick="onClick_stop_btn"
                    android:background="@drawable/circie_button"
                    android:textColor="#FF0000"
                    android:textStyle="bold"
                    android:textSize="25sp"
                    android:layout_marginLeft="75dp"
                    android:text="STOP" />


            </LinearLayout>

        </LinearLayout>

</LinearLayout>